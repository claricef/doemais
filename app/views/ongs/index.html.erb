<p id="notice"><%= notice %></p>

<h1>Ongs</h1>

<table>
  <thead>
    <tr>
      <th>Cnpj</th>
      <th>Razaosocial</th>
      <th>Nomefantasia</th>
      <th>Endereco</th>
      <th>Numero</th>
      <th>Bairro</th>
      <th>Cidade</th>
      <th>Uf</th>
      <th>Ce</th>
      <th>Email</th>
      <th>Fone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ongs.each do |ong| %>
      <tr>
        <td><%= ong.cnpj %></td>
        <td><%= ong.razaosocial %></td>
        <td><%= ong.nomefantasia %></td>
        <td><%= ong.endereco %></td>
        <td><%= ong.numero %></td>
        <td><%= ong.bairro %></td>
        <td><%= ong.cidade %></td>
        <td><%= ong.uf %></td>
        <td><%= ong.ce %></td>
        <td><%= ong.email %></td>
        <td><%= ong.fone %></td>
        <td><%= link_to 'Show', ong %></td>
        <td><%= link_to 'Edit', edit_ong_path(ong) %></td>
        <td><%= link_to 'Destroy', ong, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ong', new_ong_path %>
