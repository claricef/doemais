<p id="notice"><%= notice %></p>

<h1>Pessoasjuridicas</h1>

<table>
  <thead>
    <tr>
      <th>Cnpj</th>
      <th>Nomefantasia</th>
      <th>Razaosocial</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pessoasjuridicas.each do |pessoajuridica| %>
      <tr>
        <td><%= pessoajuridica.cnpj %></td>
        <td><%= pessoajuridica.nomefantasia %></td>
        <td><%= pessoajuridica.razaosocial %></td>
        <td><%= link_to 'Show', pessoajuridica %></td>
        <td><%= link_to 'Edit', edit_pessoajuridica_path(pessoajuridica) %></td>
        <td><%= link_to 'Destroy', pessoajuridica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

